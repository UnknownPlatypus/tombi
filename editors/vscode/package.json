{
    "name": "vscode-tomy",
    "displayName": "Tomy (TOML Toolkit)",
    "version": "0.0.1",
    "description": "A TOML language support extention backed by tomy",
    "main": "./out/main",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "npm run build-base -- --sourcemap",
        "watch": "npm run build-base -- --sourcemap --watch",
        "format": "biome format --write ./",
        "format:check": "biome format ./",
        "lint": "biome lint ./",
        "typecheck": "tsc"
    },
    "keywords": ["toml"],
    "engines": {
        "vscode": "^1.94.0"
    },
    "dependencies": {
        "vscode-languageclient": "^9.0.1"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@vscode/vsce": "^3.1.1"
    }
}
